module 'DUELink RTC'
author 'GHI Electronics'
version 1 0 
depends 'DUELink Comm' 
description 'DUELink RTC Library
'

  spec ' ' 'due_rtcw' 'rtc write _' 'auto' '1,2,3,4,5,6'
  spec 'r' 'due_rtcr' 'rtc read'

to due_rtcr {
  due_run 'dim b9[6]'
  due_run 'RtcR(b9)'
  return (due_stream_read_bytes 'b9' 6)
}

to due_rtcw data {
  due_run ('[data:join]' 'RtcW([' data '])')
}

