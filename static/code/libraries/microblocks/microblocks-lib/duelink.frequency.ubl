module 'DUELink.Frequency'
author 'GHI Electronics'
version 0 8 
depends 'http://www.duelink.com/code/libraries/microblocks/microblocks-lib/duelink.ubl' 
description 'DUELink Frequency Library'

  spec ' ' 'due_freq' 'Frequency pin _ frequency _ Hz duration _ dutycycle _ %' 'auto auto auto auto' 7 1000 500 50

to due_freq pin frequency duration dutycycle {
  due_run ('_due_templateWith' 'freq(%,%,%,%/100)' pin frequency duration dutycycle)
}

