module 'DUELink Frequency'
author 'GHI Electronics'
version 1 0 
depends 'DUELink Comm' 
description 'DUELink Frequency Library'

  spec ' ' 'due_freq' 'set frequency on pin _ frequency _ Hz duration _ ms dutycycle _ %' 'auto auto auto auto' 7 1000 500 50

to due_freq pin frequency duration dutycycle {
  due_run ('[data:join]' 'freq(' pin ',' frequency ',' duration ',' dutycycle '/' '100' ')')
}

