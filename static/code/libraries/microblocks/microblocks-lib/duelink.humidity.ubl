module 'DUELink.Humidity'
author 'GHI Electronics'
version 0 6 
depends 'http://www.duelink.com/code/libraries/microblocks/microblocks-lib/duelink.ubl' 
description 'DUELink Humidity Library'

  spec 'r' 'due_humid' 'Read humidity on pin _ type _' 'auto auto' 18 4

to due_humid pin type {
  return ('[data:convertType]' (due_runWithResult ('_due_templateWith' 'Humid(%,%)' pin type)) 'number')
}

