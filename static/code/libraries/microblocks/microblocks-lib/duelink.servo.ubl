module 'DUELink.Servo'
author 'GHI Electronics'
version 0 8 
depends 'http://www.duelink.com/code/libraries/microblocks/microblocks-lib/duelink.ubl'
description 'DUELink Servo Motor Library'

  spec ' ' 'due_servost' 'Set servo pin _ position _' 'auto auto' 1 90

to due_servost pin pos {
  due_run ('_due_templateWith' 'servost(%,%)' pin pos)
}

